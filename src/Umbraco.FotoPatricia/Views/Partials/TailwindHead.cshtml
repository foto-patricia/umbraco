@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage

<link href="/css/tailwind-elements-styles1.css" rel="stylesheet" />
<link href="/css/tailwind-elements-styles2.css" rel="stylesheet" />
  
<style type="text/tailwindcss">
    h1 {
        @@apply text-3xl my-8;
    }
    
    h2 {
        @@apply text-3xl my-8;
    }
    
    h3 {
        @@apply text-2xl my-4;
    }
    
    h4 {
        @@apply text-2xl my-4;
    }
    
    h5 {
        @@apply text-xl my-2;
    }
    
    h6 {
        @@apply text-xl my-2;
    }
    
    blockquote {
        @@apply text-center my-8 text-lg font-light italic font-serif;
    }
    
    p {
        @@apply w-full my-4 leading-relaxed tracking-normal antialiased;
    }
    
    ul {
        @@apply md:px-6 list-disc;
    }
    
    img {
        display: block;
        width: 100%;
    }
    
    body {
        @@apply font-sans;
    }
    
    a {
        @@apply text-cyan-600;
    }
</style>
<script src="/scripts/tailwind.js"></script>
<script>
    tailwind.config = {
        darkMode: 'class',
        theme: {
            extend: {
              spacing: {
                '128': '32rem',
              }
            }
          }
    }
</script>